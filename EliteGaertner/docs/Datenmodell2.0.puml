@startuml
title Datenmodell – EliteGärtner


skinparam ranksep 70

class Match {
   +count : Int
   +matchedDate : Date     
}

class User {
    +<u>userId : Int 
    +userName : String 
    +firstName : String 
    +lastName : String 
    +eMail : String 
    +passwordHash : String 
    +profileText : String 
}

class Preferences {
    +<u>preferencesID : Int 
    +dateUpdated : Date 
}

class Tags {
    +<u>tagId : Int
    +label : String
}

class HarvestUpload {
    +<u>uploadId : Int 
    +userId : int 
    +ImageUrl : String 
    +Description: String 
    +WeightKg : float 
    +quantity : Int 
    +uploadDate : Date 
}

class Rating {
    +<u>ratingId : Int
}

class Report {
  +<u>reportId : Int
  +reason : String
  +reportDate : Date
}

class LeaderboardEntry {
  +<u>entryId : Int
  +rank : Int
  +points : Int
  +period : String // Woche, Monat, etc.
}



'--- Beziehungen ---

User "1" -- "*" LeaderboardEntry : hat Rang
User "1" -- "1" Preferences : wählt/hat
User "1" -- "*" HarvestUpload : erstellt
User "1" -- "*" Report : erstellt
User -- Match : hat

Preferences "*" -- "*" Tags : enthalten

HarvestUpload "*" -- "*" Tags : enthalten
HarvestUpload "1" -- "*" Report : enthalten
HarvestUpload "n" -- "1" Rating : wählt aus
User "n" -- "1" Rating : wählt aus










@enduml